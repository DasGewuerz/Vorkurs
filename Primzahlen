#include <stdlib.h>
#include <time.h>
#include <stdio.h>
#include <math.h>
#include <stdbool.h>
void main(/* arguments */) {
  int x = 3;
  int y = 2;
  while (x <= 100 && x >= 3) {
    while (y != x && x / y * y != x && y < x){
       y++;

         if (y == x){
           printf("%i ist eine Primzahl\n",x );
           x++;
           y = 2;
           break;
         }
         if (x == x / y * y){
           x++;
           y = 2;
           break;
         }
       }


if (x = x / 2 * 2){
  x++;
}
}
}
